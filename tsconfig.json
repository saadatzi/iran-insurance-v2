{
  "compilerOptions": {
    "module": "commonjs",
    "resolveJsonModule": true,
    // "esModuleInterop": true,
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "esNext",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./src",
    "paths":{
      "@shared/*": ["Logger/*"]
    },
    "incremental": true,
  },
}
